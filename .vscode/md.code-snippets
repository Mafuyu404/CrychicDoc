        "body": [
            "::: chart k {\"title\": \"${1:K线图}\"}",
            "${2:2024-01-01}: ${3:2320.26}, ${4:2320.26}, ${5:2287.3}, ${6:2362.94}",
            "${7:2024-01-02}: ${8:2300}, ${9:2350}, ${10:2290}, ${11:2340}",
            "${12:2024-01-03}: ${13:2340}, ${14:2380}, ${15:2320}, ${16:2370}",
            ":::"
        ],
        "description": "K线图 (Candlestick chart) for stock data with OHLC format."
    },

    // ---------- Multi-Series Charts ----------
    "@Chart Multi-Series Bar": {
        "prefix": "@chart-bar-multi",
        "body": [
            "::: chart bar {\"title\": \"${1:Multi-Series Bar Chart}\", \"legend\": true}",
            "${2:Sales} | ${3:Q1}: ${4:120}, ${5:Q2}: ${6:132}, ${7:Q3}: ${8:140}, ${9:Q4}: ${10:150}",
            "${11:Profit} | ${3:Q1}: ${12:80}, ${5:Q2}: ${13:90}, ${7:Q3}: ${14:95}, ${9:Q4}: ${15:100}",
            ":::"
        ],
        "description": "Multi-series bar chart with legend."
    },

    "@Chart Multi-Series Area": {
        "prefix": "@chart-area-multi",
        "body": [
            "::: chart area {\"title\": \"${1:Multi-Series Area Chart}\", \"legend\": true, \"smooth\": true}",
            "${2:Desktop} | ${3:Jan}: ${4:120}, ${5:Feb}: ${6:132}, ${7:Mar}: ${8:140}",
