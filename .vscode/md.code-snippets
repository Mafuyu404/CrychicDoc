{
    // ===================================================================
    // =========== VITEPRESS FRONTMATTER & PAGE TEMPLATES ================
    // ===================================================================
    // if you change the prefix, you need to mofiy the documents
    // English bersion: doc/en/doc/vscode-snippets-guide.md
    // Chinese bersion: doc/zh/doc/vscode-snippets-guide.md

    "@Page Template": {
        "prefix": "@page-template",
        "body": [
            "---",
            "title: ${1:Page Title}",
            "description: ${2:Page description}",
            "layout: doc",
            "---",
            "",
            "# {{ $frontmatter.title }}",
            "",
            "$0"
        ],
        "description": "Scaffold a standard VitePress documentation page."
    },

    "@Root Section Template": {
        "prefix": "@root-template",
        "body": [
            "---",
            "root: true",
            "title: ${1:Section Title}",
            "collapsed: ${2|true,false|}",
            "layout: doc",
            "prev: false",
            "next: false",
            "---",
            "",
            "# {{ $frontmatter.title }}",
            "",
            "$0"
        ],
        "description": "Scaffold a new root section for the sidebar."
    },

    "@Frontmatter Block": {
        "prefix": "@frontmatter",
        "body": ["---", "${1:key}: ${2:value}", "---", "$0"],
        "description": "Insert a basic frontmatter block."
    },

    "@Frontmatter Complete": {
        "prefix": "@frontmatter-complete",
        "body": [
            "---",
            "title: ${1:Page Title}",
            "description: ${2:Page description}",
            "layout: ${3:doc}",
            "prev: ${4|false,true|}",
            "next: ${5|false,true|}",
            "authors:",
            "  - ${6:author1}",
            "tags:",
            "  - ${7:tag1}",
            "progress: ${8:100}",
            "state: ${9|published,preliminary,unfinished,outdated,renovating|}",
            "externalLinks:",
            "  - text: \"${10:Link Text}\"",
            "    link: \"${11:https://example.com}\"",
            "    priority: ${12:0}",
            "---",
            "",
            "# {{ $frontmatter.title }}",
            "",
            "$0"
        ],
        "description": "Generate a comprehensive frontmatter block."
    },

    // ---------- Individual Frontmatter Fields ----------

    "@fm: Title": {
        "prefix": "@fm-title",
        "body": ["title: ${1:Page Title}"],
        "description": "Frontmatter: Page title."
    },
    "@fm: Description": {
        "prefix": "@fm-description",
        "body": ["description: ${1:Page description}"],
        "description": "Frontmatter: Page description."
    },
    "@fm: Root": {
        "prefix": "@fm-root",
        "body": ["root: ${1|true,false|}"],
        "description": "Frontmatter: Whether this page is a sidebar root."
    },
    "@fm: Collapsed": {
        "prefix": "@fm-collapsed",
        "body": ["collapsed: ${1|true,false|}"],
        "description": "Frontmatter: Whether the sidebar section is collapsed."
    },
    "@fm: Hidden": {
        "prefix": "@fm-hidden",
        "body": ["hidden: ${1|true,false|}"],
        "description": "Frontmatter: Hide page from the sidebar."
    },
    "@fm: Layout": {
        "prefix": "@fm-layout",
        "body": ["layout: ${1|doc,home,page|}"],
        "description": "Frontmatter: Set VitePress page layout."
    },
    "@fm: Prev/Next": {
        "prefix": "@fm-prevnext",
        "body": ["prev: ${1|false,true|}", "next: ${2|false,true|}"],
        "description": "Frontmatter: Set previous/next page navigation."
    },
    "@fm: Authors": {
        "prefix": "@fm-authors",
        "body": ["authors:", "  - ${1:author}"],
        "description": "Frontmatter: Set page authors."
    },
    "@fm: Tags": {
        "prefix": "@fm-tags",
        "body": ["tags:", "  - ${1:tag}"],
        "description": "Frontmatter: Set page tags."
    },
    "@fm: Progress": {
        "prefix": "@fm-progress",
        "body": ["progress: ${1:100}"],
        "description": "Frontmatter: Set page completion progress."
    },
    "@fm: State": {
        "prefix": "@fm-state",
        "body": [
            "state: ${1|preliminary,unfinished,outdated,renovating,published|}"
        ],
        "description": "Frontmatter: Set the document's state."
    },
    "@fm: External Links": {
        "prefix": "@fm-externallinks",
        "body": [
            "externalLinks:",
            "  - text: \"${1:Link Text}\"",
            "    link: \"${2:https://example.com}\"",
            "    priority: ${3:0}"
        ],
        "description": "Frontmatter: Add external links with text, URL, and priority."
    },

    "@fm: Back Path": {
        "prefix": "@fm-backpath",
        "body": ["backPath: \"${1:../}\""],
        "description": "Frontmatter: Set a custom back path for the 'Back' button."
    },

    "@fm: Hero Snippets": {
        "prefix": "@fm-hero-snippets",
        "body": ["hero:", "  customSnippet: ${1|true,'custom'|}"],
        "description": "Frontmatter: Configure custom floating word snippets for the VPHero component. Use 'true' for custom.json or provide a custom filename (without .json)."
    },

    // ---------- Sidebar Tags Frontmatter ----------
    "@fm: HTTP Method": {
        "prefix": "@fm-method",
        "body": ["method: ${1|GET,POST,PUT,DELETE,PATCH|}"],
        "description": "Frontmatter: Set HTTP method for sidebar tags."
    },
    "@fm: Status": {
        "prefix": "@fm-status",
        "body": ["status: ${1|stable,beta,experimental,deprecated,draft|}"],
        "description": "Frontmatter: Set status for sidebar tags."
    },
    "@fm: Version": {
        "prefix": "@fm-version",
        "body": ["version: ${1:1.0.0}"],
        "description": "Frontmatter: Set version for sidebar tags."
    },
    "@fm: Difficulty": {
        "prefix": "@fm-difficulty",
        "body": [
            "difficulty: ${1|easy,medium,hard,beginner,intermediate,advanced,expert|}"
        ],
        "description": "Frontmatter: Set difficulty level for sidebar tags."
    },
    "@fm: Category": {
        "prefix": "@fm-category",
        "body": ["category: ${1:tutorial}"],
        "description": "Frontmatter: Set category for sidebar tags."
    },
    "@fm: Featured": {
        "prefix": "@fm-featured",
        "body": ["featured: ${1|true,false|}"],
        "description": "Frontmatter: Mark content as featured for sidebar tags."
    },
    "@fm: Priority": {
        "prefix": "@fm-priority",
        "body": ["priority: ${1|high,medium,low|}"],
        "description": "Frontmatter: Set priority for sidebar tags."
    },
    "@fm: Language": {
        "prefix": "@fm-language",
        "body": [
            "language: ${1|javascript,typescript,python,java,rust,go,php|}"
        ],
        "description": "Frontmatter: Set programming language for sidebar tags."
    },
    "@fm: Score": {
        "prefix": "@fm-score",
        "body": ["score: ${1:85}"],
        "description": "Frontmatter: Set score (0-100) for sidebar tags."
    },
    "@fm: Multiple Tags": {
        "prefix": "@fm-multiple-tags",
        "body": [
            "method: ${1|GET,POST,PUT,DELETE,PATCH|}",
            "status: ${2|stable,beta,experimental,deprecated,draft|}",
            "difficulty: ${3|easy,medium,hard,beginner,intermediate,advanced,expert|}",
            "category: ${4:tutorial}",
            "version: ${5:1.0.0}"
        ],
        "description": "Frontmatter: Add multiple sidebar tags at once."
    },

    // ===================================================================
    // ============== VITEPRESS BUILT-IN MARKDOWN FEATURES ===============
    // ===================================================================

    "@vp: Info Container": {
        "prefix": "@vp-info",
        "body": ["::: info ${1:Info Title}", "${2:Info content}", ":::"],
        "description": "VitePress info container."
    },
    "@vp: Tip Container": {
        "prefix": "@vp-tip",
        "body": ["::: tip ${1:Tip Title}", "${2:Tip content}", ":::"],
        "description": "VitePress tip container."
    },
    "@vp: Warning Container": {
        "prefix": "@vp-warning",
        "body": [
            "::: warning ${1:Warning Title}",
            "${2:Warning content}",
            ":::"
        ],
        "description": "VitePress warning container."
    },
    "@vp: Danger Container": {
        "prefix": "@vp-danger",
        "body": ["::: danger ${1:Danger Title}", "${2:Danger content}", ":::"],
        "description": "VitePress danger container."
    },
    "@vp: Details Container": {
        "prefix": "@vp-details",
        "body": [
            "::: details ${1:Click to expand}",
            "${2:Hidden content}",
            ":::"
        ],
        "description": "VitePress details (collapsible) container."
    },
    "@vp: Code Group": {
        "prefix": "@vp-codegroup",
        "body": [
            "::: code-group",
            "",
            "```${1:js} [${2:JavaScript}]",
            "${3:// JavaScript code}",
            "```",
            "",
            "```${4:ts} [${5:TypeScript}]",
            "${6:// TypeScript code}",
            "```",
            "",
            ":::"
        ],
        "description": "VitePress code group for tabbed code blocks."
    },
    "@vp: Code with Line Numbers": {
        "prefix": "@vp-codelines",
        "body": [
            "```${1:js}:line-numbers",
            "${2:// Code with line numbers}",
            "```"
        ],
        "description": "Code block with line numbers."
    },
    "@vp: Code with Highlighting": {
        "prefix": "@vp-codehighlight",
        "body": ["```${1:js}{${2:2-4,6}}", "${3:// Highlighted lines}", "```"],
        "description": "Code block with specific lines highlighted."
    },

    // ===================================================================
    // ================ CUSTOM PLUGINS & COMPONENTS ======================
    // ===================================================================

    // ---------- Dialog Plugin ----------
    "@Dialog Definition": {
        "prefix": "@dialog-def",
        "body": [
            "@@@ dialog-def#${1:my-dialog-id} {\"title\": \"${2:My Dialog}\", \"width\": ${3:600}}",
            "# ${4:Dialog Title (becomes H2)}",
            "",
            "${5:Dialog content supports **Markdown**.}",
            "",
            "@@@"
        ],
        "description": "Dialog definition block. Content is rendered as Markdown."
    },
    "@Dialog Trigger": {
        "prefix": "@dialog-trigger",
        "body": [":::dialog#${1:my-dialog-id} ${2:Click here}:::"],
        "description": "Inline trigger for a dialog."
    },
    "@Dialog Full": {
        "prefix": "@dialog-full",
        "body": [
            "@@@ dialog-def#${1:my-dialog-id} {\"title\": \"${2:My Dialog}\", \"width\": ${3:600}}",
            "# ${4:Dialog Title}",
            "",
            "${5:Dialog content...}",
            "",
            "@@@",
            "",
            "::: demo",
            "Click :::dialog#${1:my-dialog-id} ${6:here}::: to open the dialog.",
            ":::"
        ],
        "description": "Complete dialog definition and trigger inside a demo block."
    },

    // ---------- Alert Plugin ----------
    // Legacy v-alert format (kept for compatibility)
    "@Alert": {
        "prefix": "@alert",
        "body": [
            "::: ${1|v-success,v-info,v-warning,v-error|} ${2:Title}",
            "${3:Message}",
            ":::"
        ],
        "description": "Generic alert container (legacy format)."
    },
    "@Alert Success": {
        "prefix": "@alert-success",
        "body": ["::: v-success ${1:Success}", "${2:Success message}", ":::"],
        "description": "Success alert (legacy format)."
    },
    "@Alert Info": {
        "prefix": "@alert-info",
        "body": ["::: v-info ${1:Information}", "${2:Info message}", ":::"],
        "description": "Info alert (legacy format)."
    },
    "@Alert Warning": {
        "prefix": "@alert-warning",
        "body": ["::: v-warning ${1:Warning}", "${2:Warning message}", ":::"],
        "description": "Warning alert (legacy format)."
    },
    "@Alert Error": {
        "prefix": "@alert-error",
        "body": ["::: v-error ${1:Error}", "${2:Error message}", ":::"],
        "description": "Error alert (legacy format)."
    },

    // ---------- CustomAlert Plugin (New JSON-based format) ----------
    "@Custom Alert": {
        "prefix": "@custom-alert",
        "body": [
            "::: alert {\"type\": \"${1|success,info,warning,error|}\", \"title\": \"${2:Alert Title}\"}",
            "${3:Alert content}",
            ":::"
        ],
        "description": "Custom alert with JSON configuration."
    },
    "@Custom Alert Success": {
        "prefix": "@custom-alert-success",
        "body": [
            "::: alert {\"type\": \"success\", \"title\": \"${1:Success}\"}",
            "${2:Success message}",
            ":::"
        ],
        "description": "Custom success alert."
    },
    "@Custom Alert Info": {
        "prefix": "@custom-alert-info",
        "body": [
            "::: alert {\"type\": \"info\", \"title\": \"${1:Information}\"}",
            "${2:Info message}",
            ":::"
        ],
        "description": "Custom info alert."
    },
    "@Custom Alert Warning": {
        "prefix": "@custom-alert-warning",
        "body": [
            "::: alert {\"type\": \"warning\", \"title\": \"${1:Warning}\"}",
            "${2:Warning message}",
            ":::"
        ],
        "description": "Custom warning alert."
    },
    "@Custom Alert Error": {
        "prefix": "@custom-alert-error",
        "body": [
            "::: alert {\"type\": \"error\", \"title\": \"${1:Error}\"}",
            "${2:Error message}",
            ":::"
        ],
        "description": "Custom error alert."
    },
    "@Custom Alert Full": {
        "prefix": "@custom-alert-full",
        "body": [
            "::: alert {\"type\": \"${1|success,info,warning,error|}\", \"title\": \"${2:Alert Title}\", \"variant\": \"${3|flat,elevated,tonal,outlined,text,plain|}\", \"density\": \"${4|default,comfortable,compact|}\", \"border\": \"${5|start,end,top,bottom,true,false|}\", \"color\": \"${6:purple}\", \"icon\": \"${7:mdi-star}\", \"text\": \"${8:Text property content}\"}",
            "${9:Slot content}",
            ":::"
        ],
        "description": "Full-featured custom alert with all configuration options."
    },

    // ---------- Markmap Plugin ----------
    "@Markmap Basic": {
        "prefix": "@markmap",
        "body": [
            "```markmap",
            "# ${1:Main Topic}",
            "",
            "## ${2:Subtopic 1}",
            "- ${3:Item 1}",
            "- ${4:Item 2}",
            "",
            "## ${5:Subtopic 2}",
            "- ${6:Item 3}",
            "- ${7:Item 4}",
            "```"
        ],
        "description": "Basic markmap mindmap diagram."
    },
    "@Markmap Full": {
        "prefix": "@markmap-full",
        "body": [
            "```markmap",
            "# ${1:Main Topic}",
            "",
            "## ${2:Section 1}",
            "### ${3:Subsection 1.1}",
            "- ${4:Item 1}",
            "- ${5:Item 2}",
            "  - [${6:Link Text}](${7:https://example.com})",
            "",
            "### ${8:Subsection 1.2}",
            "- ${9:Item 3}",
            "- ${10:Item 4}",
            "",
            "## ${11:Section 2}",
            "- ${12:Item 5}",
            "- ${13:Item 6}",
            "```"
        ],
        "description": "Full-featured markmap with hierarchy and links."
    },

    // ---------- Other Plugins & Components ----------
    "@Carousel": {
        "prefix": "@carousel",
        "body": [
            "::: carousels#{\"cycle\": ${1|true,false|}, \"interval\": ${2:2800}}",
            "@tab",
            "![${4:Image 1}](${5:/path/to/image1.png})",
            "@tab",
            "![${6:Image 2}](${7:/path/to/image2.png})",
            ":::"
        ],
        "description": "Image carousel plugin."
    },
    "@File Tree": {
        "prefix": "@file-tree",
        "body": [
            "<LiteTree>",
            "${1:Root Folder}",
            "    ${2:Subfolder}",
            "        ${3:file.txt}      //+         ${4:Add marker}",
            "        ${5:file2.txt}     //x         ${6:Error marker}",
            "</LiteTree>"
        ],
        "description": "Basic file tree structure component."
    },
    "@File Tree Full": {
        "prefix": "@file-tree-full",
        "body": [
            "<LiteTree>",
            "// Define style variables",
            "#${1:important}=color:red;font-weight:bold;background:#ffe6e6;padding:2px 6px;border-radius:3px;",
            "#${2:success}=color:green;font-weight:bold;background:#e6ffe6;padding:2px 6px;border-radius:3px;",
            ".${3:folder}=color:#1976d2;font-weight:500;",
            "// Define icon variables",
            "${4:folder}=data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIxZW0iIGhlaWdodD0iMWVtIiB2aWV3Qm94PSIwIDAgMjQgMjQiPjxwYXRoIGZpbGw9ImN1cnJlbnRDb2xvciIgZD0iTTEwIDRIOGEyIDIgMCAwIDAtMiAydjEyYTIgMiAwIDAgMCAyIDJoOGEyIDIgMCAwIDAgMi0yVjhhMiAyIDAgMCAwLTItMmgtM2wtMi0yWiIvPjwvc3ZnPg==",
            "---",
            "[${4:folder}] {.${3:folder}}${5:Project Name}",
            "    ${6:Subfolder}     // {#${1:important}}${7:Important note}",
            "        ${8:file.txt}  // {#${2:success}}${9:Completed}",
            "</LiteTree>"
        ],
        "description": "Full-featured file tree with variables and styling."
    },

    // ---------- LiteTree Variables ----------
    "@LiteTree Style Var": {
        "prefix": "@lite-style-var",
        "body": [
            "#${1:styleName}=${2:color:red;font-weight:bold;background:#ffe6e6;padding:2px 6px;border-radius:3px;}"
        ],
        "description": "LiteTree style variable definition."
    },
    "@LiteTree Class Var": {
        "prefix": "@lite-class-var",
        "body": [".${1:className}=${2:color:#1976d2;font-weight:500;}"],
        "description": "LiteTree class variable definition."
    },
    "@LiteTree Icon Var": {
        "prefix": "@lite-icon-var",
        "body": ["${1:iconName}=data:image/svg+xml;base64,${2:iconBase64Data}"],
        "description": "LiteTree icon variable definition."
    },

    // ---------- Common Icon Variables ----------
    "@Icon: Folder": {
