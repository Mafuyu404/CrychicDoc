    // ===================================================================
    // =================== VUETIFY TIMELINE PLUGIN ======================
    // ===================================================================

    // ---------- Timeline Plugin Syntax ----------
    "@Timeline Markdown": {
        "prefix": "@timeline",
        "body": [
            "```timeline",
            "# align:${1|start,center,end|}, direction:${2|vertical,horizontal|}, density:${3|default,compact,comfortable|}",
            "",
            "## ${4:事件标题} | icon:${5:mdi-calendar}, color:${6:primary}, size:${7|default,small,large|}",
            "### ${8:时间/日期}",
            "${9:事件描述内容}",
            "${10:支持多行文本}",
            "",
            "## ${11:另一个事件} | color:${12:success}",
            "### ${13:另一个时间}",
            "${14:另一个事件的描述}",
            "```"
        ],
        "description": "创建Timeline的Markdown插件语法"
    },

    "@Timeline Simple": {
        "prefix": "@timeline-simple",
        "body": [
            "```timeline",
            "## ${1:事件标题}",
            "### ${2:时间}",
            "${3:事件描述}",
            "",
            "## ${4:另一个事件}",
            "### ${5:另一个时间}",
            "${6:另一个描述}",
            "```"
        ],
        "description": "简单的Timeline语法"
    },

    "@Timeline with Icons": {
        "prefix": "@timeline-icons",
        "body": [
            "```timeline",
            "# align:start",
            "",
            "## ${1:项目启动} | icon:mdi-rocket, color:success, size:large",
            "### ${2:2024-01-01}",
            "${3:项目正式启动，团队开始工作}",
            "",
            "## ${4:开发阶段} | icon:mdi-code-braces, color:info",
            "### ${5:2024-02-01}",
            "${6:进入核心功能开发阶段}",
            "",
            "## ${7:项目发布} | icon:mdi-check-circle, color:primary, size:large",
            "### ${8:2024-03-01}",
            "${9:项目成功发布上线}",
            "```"
        ],
        "description": "带图标的Timeline示例"
    },

    "@Timeline Horizontal": {
        "prefix": "@timeline-horizontal",
        "body": [
            "```timeline",
            "# direction:horizontal, align:center",
            "",
            "## ${1:阶段1}",
            "### ${2:Q1}",
            "${3:第一阶段内容}",
            "",
            "## ${4:阶段2}",
            "### ${5:Q2}",
            "${6:第二阶段内容}",
            "",
            "## ${7:阶段3}",
            "### ${8:Q3}",
            "${9:第三阶段内容}",
            "```"
        ],
        "description": "水平方向的Timeline"
    },

    "@Timeline Dense": {
        "prefix": "@timeline-dense",
        "body": [
            "```timeline",
            "# density:compact, align:start",
            "",
            "## ${1:任务1} | size:small, color:${2:primary}",
            "### ${3:时间1}",
            "${4:简短描述}",
            "",
            "## ${5:任务2} | size:small, color:${6:secondary}",
            "### ${7:时间2}",
            "${8:简短描述}",
            "",
            "## ${9:任务3} | size:small, color:${10:success}",
            "### ${11:时间3}",
            "${12:简短描述}",
            "```"
        ],
        "description": "紧凑密度的Timeline"
    },

    "@Timeline": {
        "prefix": "@timeline",
        "body": [
            ":::: timeline${1: density=\"${2|default,compact,comfortable|}\"}${3: direction=\"${4|vertical,horizontal|}\"}",
            "$0",
            "::::"
        ],
        "description": "Timeline容器"
    },

    "@Timeline Start": {
        "prefix": "@timeline-start",
        "body": [
            ":::: timeline",
            "$0",
            "::::"
        ],
        "description": "左对齐Timeline容器"
    },

    "@Timeline Center": {
        "prefix": "@timeline-center", 
        "body": [
            ":::: timeline",
            "$0",
            "::::"
        ],
        "description": "居中Timeline容器"
    },

    // ---------- Timeline Items (Preset Types) ----------
    // ===== Timeline项目预设 =====
