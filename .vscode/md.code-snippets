    // ---------- Pre-defined Style Sets ----------
    "@LiteTree Status Styles": {
        "prefix": "@lite-status-styles",
        "body": [
            "// Status styles",
            "#success=color:green;font-weight:bold;background:#e6ffe6;padding:2px 6px;border-radius:3px;",
            "#warning=color:orange;background:#fff3e0;padding:2px 6px;border-radius:3px;",
            "#error=color:red;font-weight:bold;background:#ffe6e6;padding:2px 6px;border-radius:3px;",
            "#info=color:blue;background:#e3f2fd;padding:2px 6px;border-radius:3px;"
        ],
        "description": "Pre-defined status styles for LiteTree."
    },
    "@LiteTree File Type Styles": {
        "prefix": "@lite-filetype-styles",
        "body": [
            "// File type styles",
            ".folder=color:#1976d2;font-weight:500;",
            ".file=color:#666;",
            ".important=font-weight:bold;color:#d32f2f;",
            ".new=color:white;background:#4caf50;padding:1px 4px;border-radius:2px;font-size:12px;",
            ".deprecated=color:white;background:#f44336;padding:1px 4px;border-radius:2px;font-size:12px;"
        ],
        "description": "Pre-defined file type styles for LiteTree."
    },
    "@Link Card": {
        "prefix": "@linkcard",
        "body": [
            "<Linkcard url=\"${1:https://example.com}\" title=\"${2:Title}\" description=\"${3:Description}\" logo=\"${4:/logo.png}\"/>"
        ],
        "description": "Link card component."
    },

    // ===================================================================
    // ===================== MARKDOWN UTILITIES ==========================
    // ===================================================================

    "@H1": {
        "prefix": "@h1",
        "body": ["# ${1:Title}"],
        "description": "H1 Title"
    },
    "@H2": {
        "prefix": "@h2",
        "body": ["## ${1:Title}"],
        "description": "H2 Title"
    },
    "@H3": {
        "prefix": "@h3",
        "body": ["### ${1:Title}"],
        "description": "H3 Title"
    },
    "@H4": {
        "prefix": "@h4",
        "body": ["#### ${1:Title}"],
        "description": "H4 Title"
    },

    "@Table": {
        "prefix": "@table",
        "body": [
            "| ${1:Header 1} | ${2:Header 2} |",
            "| :--- | :--- |",
            "| ${3:Cell 1}  | ${4:Cell 2}  |",
            "| ${5:Cell 3}  | ${6:Cell 4}  |"
        ],
        "description": "Insert a Markdown table."
    },

    "@Table of Contents": {
        "prefix": "@toc",
        "body": [
            "## Table of Contents",
            "",
            "- [${1:Section 1}](#${2:section-1})",
            "- [${3:Section 2}](#${4:section-2})"
        ],
        "description": "Insert a Table of Contents."
    },

    "@Demo Block": {
        "prefix": "@demo",
        "body": ["::: demo ${1:Demo Title}", "${2:Demo content}", ":::"],
        "description": "Demo block container for showcasing examples."
    },

    "@Spoiler": {
        "prefix": "@spoiler",
        "body": ["!!${1:hidden content}!!"],
        "description": "Hidden/spoiler text."
    },
    "@Highlight": {
        "prefix": "@mark",
        "body": ["==${1:highlighted text}=="],
        "description": "Highlighted (marked) text."
    },
    "@Insert": {
        "prefix": "@insert",
        "body": ["++${1:inserted text}++"],
        "description": "Inserted text."
    },
    "@Subscript": {
        "prefix": "@sub",
        "body": ["${1:text}~${2:subscript}~"],
        "description": "Subscript text."
    },
    "@Superscript": {
        "prefix": "@sup",
